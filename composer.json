{
    "name": "jcanales/danceschool",
    "type": "project",
    "license": "proprietary",
    "repositories":[
        {
            "type": "git",
            "url": "https://github.com/phpmetrics/SymfonyExtension"
        }
    ],
    "require": {
        "php": "^7.3",
        "ext-ctype": "*",
        "ext-iconv": "*",

        "craue/formflow-bundle": "~3.2",
        "doctrine/dbal": "~2.9",
        "doctrine/doctrine-bundle": "~2.0",
        "doctrine/doctrine-fixtures-bundle": "^3.3",
        "doctrine/orm": "~2.7",
        "easycorp/easyadmin-bundle": "~2.3",
        "knplabs/knp-menu": "~3.1",
        "knplabs/knp-menu-bundle": "~3.0",
        "php-http/guzzle6-adapter": "~2.0",
        "php-http/httplug-bundle": "~1.16",

        "symfony/browser-kit": "~5.0",
        "symfony/console": "~5.0",
        "symfony/css-selector": "~5.0",
        "symfony/dotenv": "~5.0",
        "symfony/flex": "^1.4",
        "symfony/framework-bundle": "~5.0",
        "symfony/monolog-bundle": "~3.5",
        "symfony/security-bundle": "~5.0",
        "symfony/yaml": "~5.0",

        "twbs/bootstrap-sass": "~3.4",
        "twig/extensions": "~1.5"
    },
    "require-dev": {
        "friends-of-phpspec/phpspec-code-coverage": "~4.3",
        "friendsofphp/php-cs-fixer": "~2.15",
        "fzaninotto/faker": "~1.8",
        "jangregor/phpstan-prophecy": "~0.4",
        "matthiasnoback/symfony-dependency-injection-test": "~4.0",
        "mopa/composer-bridge": "~1.3",
        "phpbench/phpbench": "~0.16",
        "phpmetrics/phpmetrics": "~2.4",
        "phpmetrics/symfony-extension": "dev-master@dev",
        "phpspec/phpspec": "~6.1",
        "phpstan/phpstan": "~0.11",
        "phpunit/php-code-coverage": "~7.0",
        "phpunit/phpunit": "~8.4",
        "roave/security-advisories": "dev-master@dev",
        "symfony/debug-bundle": "^5.0",
        "symfony/maker-bundle": "^1.11",
        "symfony/profiler-pack": "^1.0",
        "symfony/requirements-checker": "~1.1",
        "symfony/web-profiler-bundle": "^5.0",
        "symfony/webpack-encore-bundle": "^1.7"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests",
            "spec\\": "spec"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "compile": [
            "bin/console assets:install"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.0.*"
        }
    },
    "suggest": {
        "symfony/yaml": "~3.2",
        "symfony/framework-bundle":"~3.2",
        "symfony/property-access": "~3.2",
        "symfony/validator": "~3.2",
        "symfony/security":"~3.2",
        "symfony/console":"2.8.*",
        "symfony/http-foundation": "Needed to use the HTTP port.",
        "symfony/http-kernel": "Needed to use the HTTP port.",
        "symfony/stopwatch": "~3.1",
        "symfony/translation": "~3.1",

        "doctrine/annotations": "1.2.*",
        "doctrine/cache": "~1.4",

        "sonata-project/formatter-bundle": "~3.4.0",
        "raulfraile/ladybug-bundle": "~1.0",
        "egulias/listeners-debug-command-bundle": "~3.0.0",

        "leanphp/phpspec-code-coverage": "~4.2",
        "phpunit/phpcov": "~6.0",
        "mockery/mockery": "~0.9",
        "mybuilder/phpunit-accelerator": "~1.0",
        "symfony/phpunit-bridge": "^3.2",
        "felixcarmona/coveragit": "dev-master",
        "devster/ubench": "1.1.*-dev",

        "nesbot/Carbon": "~1.13",

        "justinrainbow/json-schema": "~4.0.1",
        "peekmo/jsonpath": "dev-master",

        "aws/aws-sdk-php": "~3.0",
        "liip/theme-bundle": "~1.3.0",
        "endroid/qrcode": "1.3.4",
        "elasticsearch/elasticsearch": "~2.0",
        "guzzlehttp/guzzle": "~6.3",

        "league/csv": "@stable",

        "craue/formflow-bundle": "~3.0.1",
        "craue/twigextensions-bundle": "~2.2.0",

        "zonadev/core-design-bundle": "@stable"
    }
}
