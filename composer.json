{
    "name": "jcanales/danceschool",
    "description": "Dance school development app",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-0": {
            "": "src/",
            "SymfonyStandard": "app/"
        },
        "files": ["app/AppKernel.php"],
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "autoload-dev": {
        "psr-4": { "Tests\\": "tests/" }
    },
    "repositories":[
        {
            "type": "git",
            "url": "https://github.com/phiamo/MopaBootstrapBundle"
        },
        {
            "type": "git",
            "url": "https://github.com/phpmetrics/SymfonyExtension"
        }
    ],
    "require": {
        "php": "~7.1",
        "roave/security-advisories": "dev-master",
        "symfony/symfony": "~3.3.13",
        "symfony/assetic-bundle": "~2.8.2",
        "symfony/swiftmailer-bundle": "~3.1.6",
        "symfony/monolog-bundle": "~3.1.2",
        "symfony/polyfill-iconv": "~1.6.0",
        "sensio/distribution-bundle": "~5.0.21",
        "sensio/generator-bundle": "~3.1.6",
        "sensio/framework-extra-bundle": "~5.1.1",

        "doctrine/orm": "~2.5.13",
        "doctrine/dbal": "~2.6.3",
        "doctrine/doctrine-bundle": "~1.8.1",
        "doctrine/doctrine-fixtures-bundle": "^2.3.03",

        "twig/extensions": "~1.5.1",

        "incenteev/composer-parameter-handler": "~2.1.2",

        "friendsofsymfony/user-bundle": "~2.0.2",
        "hwi/oauth-bundle": "~0.5.3",

        "mopa/bootstrap-bundle": "~3.2.1",
        "twbs/bootstrap-sass": "~3.3.7",
        "knplabs/knp-paginator-bundle": "~2.6.0",
        "knplabs/knp-menu-bundle": "~2.2.0",
        "knplabs/knp-menu": "~2.3.0",
        "misd/phone-number-bundle": "~1.2.0",

        "craue/formflow-bundle": "~3.0.1",
        "craue/twigextensions-bundle": "~2.2.0",
        "javiereguiluz/easyadmin-bundle": "~1.17.7",

        "webfactory/exceptions-bundle": "@stable",
        "sonata-project/admin-bundle": "^3.28.0",
        "sonata-project/doctrine-orm-admin-bundle": "^3.2"

    },
    "require-dev": {
        "phpspec/phpspec": "~4.2.4",

        "phpunit/phpunit": "~6.4.4",
        "phpunit/php-code-coverage": "~5.2.4",
        "matthiasnoback/symfony-dependency-injection-test": "^2.3",

        "friendsofphp/php-cs-fixer": "~2.8.3",
        "phpmetrics/phpmetrics": "~2.3.2",
        "phpmetrics/symfony-extension": "dev-master@dev",

        "fzaninotto/faker": "~1.7.1"
    },
    "suggest": {
        "symfony/yaml": "~3.2",
        "symfony/framework-bundle":"~3.2",
        "symfony/property-access": "~3.2",
        "symfony/validator": "~3.2",
        "symfony/security":"~3.2",
        "symfony/console":"2.8.*",
        "symfony/http-foundation": "Needed to use the HTTP port.",
        "symfony/http-kernel": "Needed to use the HTTP port.",
        "symfony/stopwatch": "~3.1",
        "symfony/translation": "~3.1",

        "doctrine/annotations": "1.2.*",
        "doctrine/cache": "~1.4",

        "raulfraile/ladybug-bundle": "~1.0",
        "egulias/listeners-debug-command-bundle": "~3.0.0",

        "henrikbjorn/phpspec-code-coverage": "~3.0",
        "phpunit/phpcov": ">=2.0,<3",
        "mockery/mockery": "~0.9",
        "mybuilder/phpunit-accelerator": "~1.0",
        "symfony/phpunit-bridge": "^3.2",
        "felixcarmona/coveragit": "dev-master",
        "devster/ubench": "1.1.*-dev",

        "nesbot/Carbon": "~1.13",

        "justinrainbow/json-schema": "~4.0.1",
        "peekmo/jsonpath": "dev-master",

        "aws/aws-sdk-php": "~3.0",
        "liip/theme-bundle": "~1.3.0",
        "endroid/qrcode": "1.3.4",
        "elasticsearch/elasticsearch": "~2.0",
        "guzzlehttp/guzzle": "6.2.*, >=6.2.1",

        "league/csv": "@stable",

        "zonadev/core-design-bundle": "@stable"

    },
    "scripts": {
        "compile": [
            "bin/console assets:install"
        ],
        "post-root-package-install": [

        ],
        "post-install-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget",
            "Mopa\\Bundle\\BootstrapBundle\\Composer\\ScriptHandler::postInstallSymlinkTwitterBootstrapSass",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache"
        ],
        "post-update-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget",
            "Mopa\\Bundle\\BootstrapBundle\\Composer\\ScriptHandler::postInstallSymlinkTwitterBootstrapSass"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "symfony-var-dir": "var",
        "symfony-bin-dir": "bin",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    }
}
