{
    "name": "jcanales/mayimbe",
    "description": "Project test for Mayimbe's school",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-0": {
            "": "src/",
            "SymfonyStandard": "app/"
        },
        "files": ["app/AppKernel.php"],
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "autoload-dev": {
        "psr-4": { "Tests\\": "tests/" }
    },
    "repositories":[
        {
            "type": "git",
            "url": "https://github.com/phiamo/MopaBootstrapBundle"
        }
    ],
    "require": {
        "php": ">=5.5.26",
        "symfony/symfony": "3.2.*",
        "symfony/assetic-bundle": "~2.8.1",
        "symfony/swiftmailer-bundle": "~2.5.3",
        "symfony/monolog-bundle": "~3.0.3",
        "symfony/polyfill-iconv": "~1.3.0",
        "sensio/distribution-bundle": "~5.0.18",
        "sensio/framework-extra-bundle": "~3.0,>=3.0.23",

        "doctrine/orm": "~2.5.3",
        "doctrine/dbal": "~2.5.12",
        "doctrine/doctrine-bundle": "~1.6.7",
        "doctrine/doctrine-fixtures-bundle": "^2.3",

        "twig/extensions": "~1.0",

        "incenteev/composer-parameter-handler": "~2.0",

        "friendsofsymfony/user-bundle": "~2.0@dev",

        "mopa/bootstrap-bundle": "dev-master",
        "twbs/bootstrap-sass": "dev-master",
        "knplabs/knp-paginator-bundle": "~2.5.3",
        "knplabs/knp-menu-bundle": "~2.1.3",
        "knplabs/knp-menu": "~2.2.0",
        "misd/phone-number-bundle": "~1.0",

        "craue/formflow-bundle": "dev-master",
        "javiereguiluz/easyadmin-bundle": "~1.16.5",

        "webfactory/exceptions-bundle": "@stable"
    },
    "require-dev": {
        "sensio/generator-bundle": "~3.1.3"
    },
    "suggest": {
        "liip/theme-bundle": "~1.3.0",

        "raulfraile/ladybug-bundle": "~1.0",
        "egulias/listeners-debug-command-bundle": "~3.0.0",
        "phpunit/phpunit":"~4.8.0",
        "mybuilder/phpunit-accelerator": "~1.0",
        "phpunit/phpcov": "~2.0",
        "phpspec/phpspec": "~2.4.0",
        "mockery/mockery":"0.9.*",
        "henrikbjorn/phpspec-code-coverage": "~2.0",
        "symfony/phpunit-bridge": "^3.2",
        "friendsofphp/php-cs-fixer": "~2.1.1",
        "fzaninotto/faker": "1.5.0"
    },
    "scripts": {
        "compile": [
            "bin/console assets:install"
        ],
        "post-root-package-install": [
            "SymfonyStandard\\Composer::hookRootPackageInstall"
        ],
        "post-install-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget",
            "Mopa\\Bundle\\BootstrapBundle\\Composer\\ScriptHandler::postInstallSymlinkTwitterBootstrapSass",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache"
        ],
        "post-update-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget",
            "Mopa\\Bundle\\BootstrapBundle\\Composer\\ScriptHandler::postInstallSymlinkTwitterBootstrapSass"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "symfony-var-dir": "var",
        "symfony-bin-dir": "bin",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    }
}
