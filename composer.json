{
    "name": "jcanales/danceschool",
    "description": "Dance school development app",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "repositories":[
        {
            "type": "git",
            "url": "https://github.com/phiamo/MopaBootstrapBundle"
        },
        {
            "type": "git",
            "url": "https://github.com/phpmetrics/SymfonyExtension"
        }
    ],
    "require": {
        "php": "^7.2",
        "sensio/framework-extra-bundle": "^5.1",
        "symfony/flex": "^1.2",
        "symfony/console": "^4.2",
        "symfony/framework-bundle": "^4.2",
        "symfony/orm-pack": "^1.0",
        "symfony/yaml": "^4.2",
        "symfony/swiftmailer-bundle": "~3.2.6",
        "symfony/monolog-bundle": "~3.3.1",
        "symfony/polyfill-iconv": "~1.11.0",
        "symfony/security-acl": "~3.0",
        "symfony/security-core": "~4.2",

        "doctrine/orm": "~2.6.3",
        "doctrine/dbal": "~2.9.2",
        "doctrine/doctrine-bundle": "~1.10",
        "doctrine/doctrine-fixtures-bundle": "^3.1",

        "twig/extensions": "~1.5.1",

        "friendsofsymfony/user-bundle": "dev-master@dev",
        "hwi/oauth-bundle": "~0.6.0",
        "php-http/guzzle6-adapter": "~1.1",
        "php-http/httplug-bundle": "~1.8",

        "mopa/bootstrap-bundle": "~3.3",
        "twbs/bootstrap-sass": "~3.4.1",
        "craue/formflow-bundle": "~3.1",
        "knplabs/knp-paginator-bundle": "~3.0",
        "knplabs/knp-menu-bundle": "~2.2.0",
        "knplabs/knp-menu": "~2.3.0",
        "misd/phone-number-bundle": "dev-master@dev",

        "javiereguiluz/easyadmin-bundle": "~2.1",
        "sonata-project/admin-bundle": "^3.28.0",
        "sonata-project/doctrine-orm-admin-bundle": "^3.3"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "symfony/browser-kit": "^4.2",
        "symfony/css-selector": "^4.2",
        "symfony/debug-bundle": "^4.2",
        "symfony/dotenv": "^4.0",
        "symfony/web-profiler-bundle": "^4.2",
        "symfony/maker-bundle": "^1.11",

        "fzaninotto/faker": "~1.8.0",
        "friendsofphp/php-cs-fixer": "~2.14.2",
        "matthiasnoback/symfony-dependency-injection-test": "^4.0.0",
        "phpspec/phpspec": "~5.1.0",
        "phpunit/php-code-coverage": "~7.0.3",
        "phpunit/phpunit": "~8.1.3",

        "phpmetrics/phpmetrics": "~2.4.1",
        "phpmetrics/symfony-extension": "dev-master@dev"
    },
    "suggest": {
        "symfony/yaml": "~3.2",
        "symfony/framework-bundle":"~3.2",
        "symfony/property-access": "~3.2",
        "symfony/validator": "~3.2",
        "symfony/security":"~3.2",
        "symfony/console":"2.8.*",
        "symfony/http-foundation": "Needed to use the HTTP port.",
        "symfony/http-kernel": "Needed to use the HTTP port.",
        "symfony/stopwatch": "~3.1",
        "symfony/translation": "~3.1",

        "doctrine/annotations": "1.2.*",
        "doctrine/cache": "~1.4",

        "sonata-project/formatter-bundle": "~3.4.0",
        "raulfraile/ladybug-bundle": "~1.0",
        "egulias/listeners-debug-command-bundle": "~3.0.0",

        "henrikbjorn/phpspec-code-coverage": "~3.0",
        "phpunit/phpcov": ">=2.0,<3",
        "mockery/mockery": "~0.9",
        "mybuilder/phpunit-accelerator": "~1.0",
        "symfony/phpunit-bridge": "^3.2",
        "felixcarmona/coveragit": "dev-master",
        "devster/ubench": "1.1.*-dev",

        "nesbot/Carbon": "~1.13",

        "justinrainbow/json-schema": "~4.0.1",
        "peekmo/jsonpath": "dev-master",

        "aws/aws-sdk-php": "~3.0",
        "liip/theme-bundle": "~1.3.0",
        "endroid/qrcode": "1.3.4",
        "elasticsearch/elasticsearch": "~2.0",
        "guzzlehttp/guzzle": "6.2.*, >=6.2.1",

        "league/csv": "@stable",

        "craue/formflow-bundle": "~3.0.1",
        "craue/twigextensions-bundle": "~2.2.0",

        "zonadev/core-design-bundle": "@stable"

    },
    "scripts": {
        "compile": [
            "bin/console assets:install"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "config": {
        "bin-dir": "bin",
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony-app-dir": "src",
        "symfony-web-dir": "public",
        "symfony-var-dir": "var",
        "symfony-bin-dir": "bin",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "allow-contrib": false
    }
}
